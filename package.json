{
  "name": "swapi-typescript",
  "version": "0.0.1",
  "description": "Typescript Client of SWAPI",
  "main": "dist/index.js",
  "jsnext:main": "dist/index.esm.js",
  "module": "dist/index.esm.js",
  "sideEffects": false,
  "srctype": "ts",
  "scripts": {
    "rename": "node rename.js",
    "clean": "rimraf ./dist",
    "lint": "eslint -c .eslintrc.js 'src/**/*.ts'",
    "lint:fix": "eslint -c .eslintrc.js 'src/**/*.ts' --fix",
    "build:self": "rollup -c config/rollup.config.js",
    "build:esm": "rollup -c config/rollup.config.esm.js",
    "build:aio": "rollup -c config/rollup.config.aio.js",
    "build:aio.min": "cross-env NODE_ENV=production npm run build:aio",
    "build": "npm run clean && npm run build:self && npm run build:esm && npm run build:aio && npm run build:aio.min",
    "test": "npm run lint && nyc --nycrc-path .nycrc-ts mocha",
    "coveralls": "nyc --nycrc-path .nycrc-ts report --reporter=text-lcov | coveralls",
    "release": "npm test && npm run build && git commit -am $npm_package_version && git push && git tag $npm_package_version && git push --tags"
  },
  "author": "Jack",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/mrzzcn/swapi-typescript.git"
  },
  "bugs": {
    "url": "https://github.com/mrzzcn/swapi-typescript/issues"
  },
  "devDependencies": {
    "@babel/cli": "7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "7.5.5",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "babel-plugin-istanbul": "5.2.0",
    "cdkit": "1.3.0",
    "coveralls": "3.0.6",
    "cross-env": "5.2.0",
    "es5-shim": "4.5.13",
    "eslint": "^6.2.1",
    "mocha": "6.2.0",
    "nyc": "14.1.1",
    "rimraf": "3.0.0",
    "rollup": "1.19.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "10.0.2",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-typescript2": "^0.22.1",
    "rollup-plugin-uglify": "6.0.2",
    "should": "^13.2.3",
    "should-sinon": "0.0.6",
    "sinon": "^7.4.1",
    "source-map-support": "0.5.13",
    "ts-node": "8.3.0",
    "tslib": "1.10.0",
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "@jsmini/extend": "0.3.1",
    "@jsmini/type": "0.8.0"
  }
}
